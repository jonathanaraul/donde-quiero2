<!-- destacados Section -->
	<div id="destacados">
		<h2 > Destacados </h2>
		<div class="clearfix"></div>
		{%for element in destacados%}
		<div class="item">
			<a href="{{ path('proyecto_principal_evento_individual',{'id':element.id}) }}">
				<img src="{{ asset('uploads/servicios/'~ element.path  ) }}" alt="image 1" style="width:180px;height:135px;">
			</a><!-- Image must be 400px by 300px -->
			<h3>{{element.nombre|title}}</h3><!--Title-->
			<ul>
				<li class="izquierda">
					Categor√≠a
				</li>
				<li class="izquierda"></li>
				<li class="derecha">
					{{element.precioPorHora}} &euro;
				</li>
			</ul><!--Category-->
		</div>
		<!--/item-->
		{%endfor%}

		<div class="clearfix"></div>
	</div>
<!-- destacados Section End -->